<UserControl x:Class="PropertiesReplicator.CheckboxItemUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PropertiesReplicator"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="300">
    <Grid>
        <CheckBox x:Name="selectedClassCheckbox" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="classNameTextBlock" HorizontalAlignment="Left" Margin="20,0,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
